<%= form_for [@project,@user_project] do |f| %>
  <div class="row">
    <div class="col-md-4 offset-4">
      <h3>Assign Project to Developers and Qa's</h3>
      <br>
      <%= f.label :developer %>
      <%= f.collection_check_boxes :user_id, User.where(user_type: 1).order(:id), :id, :name %>
      <br>
      <br>
      <%= f.label :qa %>
      <%= f.collection_check_boxes :user_id, User.where(user_type: 2).order(:id), :id, :name %>
      <br>
      <%= f.submit "Assign", :class => 'btn btn-dark btn-sm' %>
    </div>
  </div>
  <br>
<% end %>
